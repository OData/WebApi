<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>Microsoft.AspNetCore.OData</id>
    <title>Microsoft ASP.NET Core 2.0 and 3.x for OData v4.0</title>
    <version>$VersionNuGetSemantic$</version>
    <authors>Microsoft</authors>
