{
  "webroot": "wwwroot",

  "dependencies": {
    "Microsoft.AspNet.Mvc": "6.0.0-beta7-*",
    "Microsoft.AspNet.Server.IIS": "1.0.0-beta7-*",
    "Microsoft.AspNet.Server.WebListener": "1.0.0-beta7-*",
    "Microsoft.AspNet.StaticFiles": "1.0.0-beta7-*",
    "Microsoft.AspNet.OData": "6.0.0-*"
  },

  "commands": {
    "web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls http://localhost:5000",
    "dev": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls http://localhost:5000 --hosting:environment development"
  },

  "frameworks": {
    "dnx451": { },
    "dnxcore50": { }
  },

  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ],
  "publishExclude": [
    "node_modules",
    "bower_components",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ]
}
